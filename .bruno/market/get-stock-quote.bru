meta {
  name: Get Stock Quote
  type: http
  seq: 1
}

get {
  url: {{baseUrl}}/market/stock/AAPL
  body: none
}

query {
  period: 1mo
  interval: 1d
}

tests {
  test("should return 200", function() {
    expect(res.status).to.equal(200);
  });

  test("should have ticker in response", function() {
    expect(res.body.ticker || res.body.symbol).to.exist;
  });
}
